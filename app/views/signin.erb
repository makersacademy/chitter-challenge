<html>
  <head>
    <title>Chitter</title>
    <link rel="stylesheet" type="text/css" href="<%= url("/css/styles.css")%>"/>
  </head>
  <body>
    <div class="nav">
      <img
        src="https://external-content.duckduckgo.com/iu/?u=http%3A%2F%2Fwww.clipartbest.com%2Fcliparts%2FncB%2FK8g%2FncBK8gGAi.png&f=1&nofb=1"
        alt="chitter logo"
        class="bird"
      />
    </div>

    <div class="page">
      <div class="login-window">
        <div class="signup-container">
          <form action="/users/signin" method="post" class="signup-form">
            <input type="input" name="email" placeholder="email@gmail.com" />
            <input
              type="password"
              name="password"
              placeholder="super secret password"
            />
            <input type="submit" value="Sign in" />
          </form>
        </div>
      </div>
    </div>
  </body>
</html>
