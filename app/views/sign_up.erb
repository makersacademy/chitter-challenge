<h1> Sign up for Chitter!</h1>
  <form action='/sign-up' method='POST'>
    <div>
      <label>
      EMAIL
      <input type='text' name='email'/>
    </label>
    </div>

    <div>
      <label>
      NAME
      <input type='text' name='name'/>
      </label>
    </div>

    <div>
      <label>
      USERNAME
      <input type='text' name='username'/>
      </label>
    </div>

    <div>
      <label>
      PASSWORD
      <input type='text' name='password'/>
      </label>
    </div>

    <input type='submit' value='Sign Up'/>
  </form>
