<!DOCTYPE html>
<html>
  <head>
    <title>Peeps</title>
  </head>
  <body>
    <h1> Peeps </h1>
      <ul id='peeps'>
        <% @peeps.reverse.each do |peep| %>
          <li>
            <%= peep.body %>
            <br></br>
            <%= peep.time_stamp %>
            <br></br>
          </li>
        <% end %>
  </body>
</html>
