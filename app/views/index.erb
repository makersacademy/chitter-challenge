<!DOCTYPE html>
  <head>
  </head>

<h1>Welcome, <%=current_user.email%></h1>

<a href="/peeps/new">Add peep</a><br>
<a href="/user/new">Create an account</a><br>
<a href="/sessions/new">Sign in</a>

<ul id="peeps">
  <% @peeps.each do |peep| %>
    <li>
      <%= peep.message %> <br>
      User: <%=peep.user.username%>, <%=peep.user.name%><br>
      <%= peep.added_at %>
    </li>
  <% end %>
</ul>

</html>
