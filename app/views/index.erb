<h1>Welcome to Chitter</h1>

<form action='users/sign_up'>
  <button type="submit"> Sign up </button>
</form>

<form action='sessions/new'>
  <button type="submit"> Sign in </button>
</form>

<ul>
  <% @peeps.reverse.each do |peep| %>
    <li>
      <%= peep.time %><br>
      <%= peep.message %>
    </li>
  <% end %>
</ul>
