<h1> Peeps </h1>
<ul id='peeps'>
  <% @peeps.each do |peep| %>
    <li>
      @<%= peep.user.user_id %>
       <%= peep.created_at.strftime('on %Y/%m/%d at %I:%M:%S %p')  %><br>
      <%= peep.message %> <br>
    </li><br>
  <% end %><br>
</ul>
<form action="/peeps/new" method="get">
  <input type="submit" value="Click to peep">
</form>
