<h1>Chitter</h1>

<p><a href="/users/new">Sign up</a></p>

<p><form method='GET' action='/peeps/new'>
	<input type="submit" value="Add Peep">
</form></p>



<p><form method='GET' action='/sessions/new'>
	<input type="submit" value="Existing User? Sign in">
</form></p>

<ul id="peeps">
	<% @peeps.each do |peep| %>
	<li>
		Title: <%= peep.title   %>
		Peep:  <%= peep.message %>
	</li>
<% end %>
</ul>