<link rel="stylesheet" href="../style.css" type="text/css" />
<link href='http://fonts.googleapis.com/css?family=Open+Sans' rel='stylesheet' type='text/css'>


<h1> Peeps </h1>

<ul id="peeps">
  <% @peeps.each do |peep| %>
    <li>
				User: <%= peep.user.username %>
	  		Content: <%= peep.content %>
	  		Time: <%= peep.time %>
		</li>
  <% end %>
</ul>
