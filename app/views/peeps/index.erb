<h2>Peeps</h2>

<ul id='peeps'>
  <% @peeps.each do |peep| %>
    <li>
      Message: <%= peep.message %>
      Handle: <%= peep.user.user_name %>
      Name:   <%= peep.user.name %>
    </li>
  <% end %>
</ul>
<form action='/peeps/new' method="get">
  <button type='submit' class="btn">
  Create Peep!
</button>
</form>
