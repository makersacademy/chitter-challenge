<!DOCTYPE html>
<html>
<head> <%= erb :'head' %> </head>

<h2> Peeps </h2>
<div style="margin:auto;width:70%;padding: 50px;text-align: center;">
<ul id='peeps'>
  <% @peeps.each do |peep| %>
    <li>
      <%= peep.message   %>
    </li>
  <% end %>
</ul>

<div>
<form action="/peeps/new" method="get" >
  <input type="hidden" >
   <input type="submit" value="New peep">
 </form>
</div>
</div>
</html>
