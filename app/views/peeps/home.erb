
<h1> Peeps! </h1>

<form action='/sessions/sign_in'>
  <input type='submit' value='Sign in'>
</form>

<form action='/users/new_user'>
  <input type='submit' value='Sign up'>
</form>

<ul id='peeps'>
  <% @peeps.each do |peep| %>
    <li>
      Title: <%= peep.title %>
      Message:   <%= peep.message %>
    </li>
  <% end %>
</ul>
