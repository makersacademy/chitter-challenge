<div id='chitfeed'>
  <table width="50%">
    <tr>
      <th width="70%">Peep</th>
      <th>Time created</th>
      <th>User</th>
    </tr>
    <% @peeps.each do |peep| %>
    <tr>
      <td><%= peep.body %></td>
      <td><%= peep.created_at.to_s.gsub(/[T]/,' ')[0, 19]  %></td>
      <td>(Username)</td>
    </tr>
    <% end %>
  </table>
</div>
