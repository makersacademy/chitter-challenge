<html>
  <h2>PEEPS</h2>

  <form method="get" action="/new_peep">
    <input type="submit" value="Create a peep" />
  </form>

  <ul>
    <% @peeps.reverse.each do |x| %>
    <li>
      <%= x.author %>
      <%= x.time %>
      <%= x.peep %>
    </li>
    <% end %>
    <br />
    <br />
  </ul>

</html>
