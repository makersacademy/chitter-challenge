CREATE DATABASE chitter_test;
CREATE TABLE peeps (id SERIAL PRIMARY KEY, message VARCHAR(255), time TIMESTAMP DEFAULT CURRENT_TIMESTAMP);
ALTER TABLE peeps ADD COLUMN name VARCHAR(60);
CREATE TABLE users (id SERIAL PRIMARY KEY, name VARCHAR(30), email VARCHAR(30), password VARCHAR(30),);

CREATE DATABASE chitter;
CREATE TABLE peeps (id SERIAL PRIMARY KEY, message VARCHAR(255), time TIMESTAMP DEFAULT CURRENT_TIMESTAMP);
ALTER TABLE peeps ADD COLUMN name VARCHAR(60);
CREATE TABLE users (id SERIAL PRIMARY KEY, name VARCHAR(30), email VARCHAR(30), password VARCHAR(30),);