alias client="User"
alias controller="Controller"
alias model="Peep class"
alias data="User database"
alias view="Homepage"

client->controller: "get /"
controller->model: "requests all peeps"
model->data: "SELECT * FROM peeps\n ORDER BY timestamp DESC"
data->model: "returns entries"
model-->controller: "returns list of Peep objects"
controller->view: "requests page filled with peeps"
view-->controller: "fills page with peepes"
controller->client: "serves page"