<h1>Chitter</h1>
Hi, <%= session[:username] %>!
<form action='/peep' method='get'>
  <input type='submit' value='Peep'>
</form>

<form action="/logout" method="post">
  <input type="submit" value="Sign Out" />
</form>
<ul>
  <% @peeps.each do |peep| %>
    <p><%= peep.username %> says:
      <%= peep.peep %><br>
      <%= peep.time %>
    </p>
  <% end %>
</ul>
