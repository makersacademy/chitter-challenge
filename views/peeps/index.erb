<div class="archie-header">
  <h4> Welcome to Chitter! </h4>
  Latest Peeps:
</div>


<ul class="trwh-list-group">
  <% @peeps.reverse.each do |peep| %>
    <li class="list-group-item"><b><%= peep.username %>:</b> <%= peep.content %> <br><i>Date: <%= peep.created_at %></i></li>
  <% end %>
</ul>
