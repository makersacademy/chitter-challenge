<div class="sign-up-form">
<button class="submit-button" href="/peeps/new"> Post Peep</button>
</div>
<div class="peeps">
  <ul style="list-style: none;">
    <% @peeps.reverse.each do |peep| %>
      <li><%= peep.content %></li>
      <li style="font-size: 70%" ><%= peep.time_stamp %></li><br>
    <% end %>
  </ul>
</div>

<%# strftime('%m/%d/%Y') %>
<%# strftime('%H:%M-%m/%d/%Y') %>