<html>
  <head></head>
  <body>
<form method="get" action="/peeps">

    <h1>Peeps</h1>
      <% @peeps.each do |peep| %>
      <div>
      ID: <%= peep.id %>
      </div>
      <div>
      Peep: <%= peep.content %>
      </div>
      <div>
      Time: <%= peep.time %>
      <%end%>
    </div>
    </form>
  </body>
</html>
