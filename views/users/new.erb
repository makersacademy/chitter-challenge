<form action="/users" method="post">
  <label for="email">
    Email:
  <input type="email" name="email" />
  </label> <br>
  <label for="password">
    Password:
  <input type="password" name="password" />
  </label> <br>
  <label for="name">
    Full name:
  <input type="text" name="name" />
  </label> <br>
  <label for="username">
    Username:
  <input type="text" name="username" />
  </label> <br>
  <input type="submit" value="Sign Up" />
</form>
