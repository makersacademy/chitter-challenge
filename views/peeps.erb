<h1> Hello! You are on Chitter </h1>
<form action="/peeps" method="post">
  <input type="text" name="peep"/>
  <button type="submit" name="submit">
    Submit
  </button>
</form>

<ul>
  <% @peeps.each do |peep| %>
      <li>
        Peep:
        <%= peep.peep %>
        Date:
        <%= peep.date %>
      </li>
    <% end %>
</ul>
