<html>
  <head></head>
  <body>
    <h1>Chitter</h1>
    <div>
      <a href="">Login</a>
      <a href="">New peep</a>
    </div>
    <% @peeps.each do |peep| %>
      <div>
        <p><%= peep['content'] %></p>
        <p>Posted: <%= peep['timestamp'] %></p>
        <p>By: <%= peep['name'] %> - <%= peep['username'] %></p>
      </div>
    <% end %>
  </body>
</html>